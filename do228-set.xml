        <splash type="bool">true</splash>
        <path>Previews/do228-5.png</path>
      </preview>
      <preview>
        <type>exterior</type>
        <splash type="bool">true</splash>
        <path>Previews/do228-6.png</path>
      </preview>
    </previews>

    <tags>
      <tag>1980s</tag>
      <tag>1990s</tag>
      <tag>2000s</tag>
      <tag>2010s</tag>
      <tag>dornier</tag>
      <tag>transport</tag>
      <tag>retractable-gear</tag>
      <tag>high-wing</tag>
      <tag>ifr</tag>
      <tag>turboprop</tag>
      <tag>stol</tag>
      <tag>twin-engine</tag>
      <tag>variable-pitch</tag>
      <tag>compositor</tag>
      <tag>HDR</tag>
    </tags>
		
