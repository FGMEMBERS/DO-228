<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <chase-distance-m              type="double" archive="y"> -20 </chase-distance-m>

  <view                          n="100">
    <name                        type="string">CoPilote</name>
    <type>lookfrom</type>
    <internal                    archive="y">true</internal>
    <config>
      <from-model                type="bool">true</from-model>
      <from-model-idx            type="int">0</from-model-idx>
      <ground-level-nearplane-m  type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m                archive="y">  0.36073 </x-offset-m> <!-- Right -->
      <y-offset-m                archive="y"> -0.30474 </y-offset-m> <!-- Up    -->
      <z-offset-m                archive="y"> -4.76123 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg          type="double">-10</pitch-offset-deg>
      <default-field-of-view-deg type="double">90</default-field-of-view-deg>
    </config>
  </view>

  <view                          n="101">
    <name                        type="string">Passanger seat #1</name>
    <type>lookfrom</type>
    <internal                    archive="y">true</internal>
    <config>
      <from-model                type="bool">true</from-model>
      <from-model-idx            type="int">0</from-model-idx>
      <ground-level-nearplane-m  type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m                archive="y">  0.43927 </x-offset-m> <!-- Right -->
      <y-offset-m                archive="y"> -0.58474 </y-offset-m> <!-- Up    -->
      <z-offset-m                archive="y"> -2.93123 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg          type="double">-10</pitch-offset-deg>
      <default-field-of-view-deg type="double">90</default-field-of-view-deg>
    </config>
  </view>

  <view                          n="102">
    <name                        type="string">Passanger seat #2</name>
    <type>lookfrom</type>
    <internal                    archive="y">true</internal>
    <config>
      <from-model                type="bool">true</from-model>
      <from-model-idx            type="int">0</from-model-idx>
      <ground-level-nearplane-m  type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m                archive="y"> -0.42073 </x-offset-m> <!-- Right -->
      <y-offset-m                archive="y"> -0.57474 </y-offset-m> <!-- Up    -->
      <z-offset-m                archive="y"> -0.27123 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg          type="double">-10</pitch-offset-deg>
      <default-field-of-view-deg type="double">90</default-field-of-view-deg>
    </config>
  </view>

  <view                          n="103">
    <name                        type="string">Tail Camera View</name>
    <type>lookfrom</type>
    <internal                    archive="y">true</internal>
    <config>
      <from-model                type="bool">true</from-model>
      <from-model-idx            type="int">0</from-model-idx>
      <ground-level-nearplane-m  type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m                archive="y"> 0.000 </x-offset-m> <!--Right-->
      <y-offset-m                archive="y"> 5.032 </y-offset-m> <!--Up-->
      <z-offset-m                archive="y"> 8.535 </z-offset-m> <!--Back-->
      <pitch-offset-deg          type="double">-5</pitch-offset-deg>
      <default-field-of-view-deg type="double">50</default-field-of-view-deg>
    </config>
  </view>

  <view                          n="0">
    <name                        type="string">Cockpit View</name>
    <internal                    archive="y">true</internal>
    <config>
      <x-offset-m                archive="y"> -0.36073 </x-offset-m> <!-- Right -->
      <y-offset-m                archive="y"> -0.30474 </y-offset-m> <!-- Up    -->
      <z-offset-m                archive="y"> -4.76123 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg          type="double">-10</pitch-offset-deg>
      <default-field-of-view-deg type="double">90</default-field-of-view-deg>
    </config>
  </view>

  <view                          n="1">
    <config>
      <target-z-offset-m         archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view                          n="2">
    <config>
      <target-z-offset-m         archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view                          n="3">
