<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

<path>ittgauge.ac</path>

<animation>
<object-name>ITTBezel</object-name>
<object-name>ITTFace</object-name>
<object-name>ITT.needle</object-name>
</animation>

<!--Working For Rembrandt-->

<animation>
<object-name>vitre</object-name>
</animation>

<animation>
<type>noshadow</type>
<object-name>vitre</object-name>
</animation>

<animation>
<type>shader</type>
<shader>chrome</shader>
<texture>Aircraft/Generic/Effects/glass_shader.png</texture>
<object-name>vitre</object-name>
</animation>
<!--Working For Rembrandt-->

<animation>
<type>material</type>
<object-name>ITTFace</object-name>
<object-name>ITT.needle</object-name>
<condition>
<property>/controls/lighting/instrument-lights</property>
</condition>
<emission>
<red>   0.50 </red>
<green> 0.25 </green>
<blue>  0.25 </blue>
</emission>
</animation>

<animation>
<type>rotate</type>
<object-name>ITT.needle</object-name>
<property>engines/engine[0]/torque-ftlb</property>
<interpolation>
<entry><ind>     0 </ind><dep>   0 </dep></entry>
<entry><ind> 1370 </ind><dep> 180 </dep></entry>
<entry><ind> 2700 </ind><dep> 280 </dep></entry>
</interpolation>
<axis>
<x> -1 </x>
<y>  0 </y>
<z>  0 </z>
</axis>
</animation> 

</PropertyList>

