    <axis>
      <x1-m> -0.42248 </x1-m>
      <y1-m>  3.41976 </y1-m>
      <z1-m> -0.18420 </z1-m>
      <x2-m> -0.42248 </x2-m>
      <y2-m>  4.61025 </y2-m>
