    <center>
      <x-m> -0.59273 </x-m>
      <y-m> -1.14988 </y-m>
      <z-m> -1.43179 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <name>TrainDroit</name>
    <object-name>axeDH</object-name>
    <object-name>axeDB</object-name>
    <object-name>roueD</object-name>
