  <!-- L'hélice gauche -->
  <animation>
    <name>HeliceCompleteGauche</name>
    <object-name>heliceG</object-name>
    <object-name>bolG</object-name>
    <object-name>propdiscG</object-name>
    <object-name>propblurG</object-name>
  </animation>

  <!-- Séléction de l'objet visible en fonction des rpm -->
  <animation>
    <type>select</type>
    <object-name>propdiscG</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>900</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblurG</object-name>
    <condition>
    <and>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>300.000</value>
      </greater-than>
      <less-than>
        <property>engines/engine[0]/rpm</property>
        <value>900.001</value>
      </less-than>
    </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>heliceG</object-name>
    <condition>
      <less-than>
        <property>engines/engine[0]/rpm</property>
        <value>900</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>HeliceCompleteGauche</object-name>
    <condition>
      <!-- <not><property>controls/engines/engine[0]/cutoff</property></not> -->
      <not><property>sim/multiplay/generic/bool[8]</property></not>
    </condition>
    <!-- <property>engines/engine[0]/n1</property> -->
    <property>sim/multiplay/generic/float[4]</property>
    <factor> 2 </factor>
    <center>
      <x-m> -3.06092 </x-m>
      <y-m> -2.52204 </y-m>
      <z-m> -0.01975 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <!-- L'hélice droite -->
  <animation>
    <name>HeliceCompleteDroite</name>
    <object-name>heliceD</object-name>
    <object-name>bolD</object-name>
    <object-name>propdiscD</object-name>
    <object-name>propblurD</object-name>
  </animation>

  <!-- Séléction de l'objet visible en fonction des rpm -->
  <animation>
    <type>select</type>
    <object-name>propdiscD</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[1]/rpm</property>
        <value>900</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblurD</object-name>
    <condition>
    <and>
      <greater-than>
        <property>engines/engine[1]/rpm</property>
        <value>300.000</value>
      </greater-than>
      <less-than>
        <property>engines/engine[1]/rpm</property>
        <value>900.001</value>
      </less-than>
    </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>heliceD</object-name>
    <condition>
      <less-than>
        <property>engines/engine[1]/rpm</property>
        <value>900</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>HeliceCompleteDroite</object-name>
    <condition>
      <!-- <not><property>controls/engines/engine[1]/cutoff</property></not> -->
      <not><property>sim/multiplay/generic/bool[9]</property></not>
    </condition>
    <!-- <property>engines/engine[1]/n1</property> -->
    <property>sim/multiplay/generic/float[5]</property>
    <factor> 2 </factor>
    <center>
      <x-m> -3.06092 </x-m>
      <y-m>  2.52204 </y-m>
      <z-m> -0.01975 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <!-- Gouverne de direction -->
  <animation>
    <type>rotate</type>
    <object-name>direction</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <factor> -15 </factor>
    <axis>
      <x1-m>  7.19136 </x1-m>
      <y1-m>  0.00000 </y1-m>
      <z1-m> -0.20213 </z1-m>
      <x2-m>  7.28855 </x2-m>
      <y2-m>  0.00000 </y2-m>
      <z2-m>  1.79220 </z2-m>
    </axis>
  </animation>

  <!-- Gouverne de profondeur -->
  <animation>
    <type>rotate</type>
    <object-name>profondeur</object-name>
    <property>surface-positions/elevator-pos-norm</property>
