<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

    <number-views type="int" archive="y">6</number-views>

  <chase-distance-m type="double" archive="y"> -20 </chase-distance-m>

 

    <view n="100">
    <name>Copilot</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">  0.36073 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y"> -0.30474 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -4.76123 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> -10 </pitch-offset-deg>
        <default-pitch-deg type="double">-10</default-pitch-deg>
      <default-field-of-view-deg> 90 </default-field-of-view-deg>
    </config>
  </view>

  <view n="101">
    <name>OPS Cam</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">  0.20 </x-offset-m> <!--Right-->
      <y-offset-m archive="y"> -1.81 </y-offset-m> <!--Up-->
      <z-offset-m archive="y"> -3.19 </z-offset-m> <!--Back-->
        <pitch-offset-deg>-10</pitch-offset-deg>
        <default-pitch-deg type="double">-10</default-pitch-deg>
      <default-field-of-view-deg type="double"> 50 </default-field-of-view-deg>
    </config>
  </view>

  
  <view n="0">
    <internal archive="y">true</internal>
    <config>
      <x-offset-m archive="y"> -0.36073 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y"> -0.30474 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -4.76123 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> -10 </pitch-offset-deg>
      <default-field-of-view-deg> 90 </default-field-of-view-deg>
    </config>
  </view>

  

 
  
  <view n="1">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="2">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="3">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="4">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="5">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="6">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  
 
   
  
  
</PropertyList>
